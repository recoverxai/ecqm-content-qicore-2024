   <name value="GlobalMalnutritionCompositeFHIR"></name>
   <title value="Global Malnutrition Composite ScoreFHIR"></title>
   <status value="draft"></status>
   <experimental value="false"></experimental>
   <date value="2024-05-20T14:14:18+00:00"></date>
   <publisher value="Academy of Nutrition and Dietetics"></publisher>
