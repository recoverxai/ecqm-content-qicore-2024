   <title value="HIV ScreeningFHIR"></title>
   <status value="draft"></status>
   <experimental value="false"></experimental>
   <date value="2024-05-04T00:21:16+00:00"></date>
   <publisher value="Centers for Disease Control and Prevention (CDC)"></publisher>
