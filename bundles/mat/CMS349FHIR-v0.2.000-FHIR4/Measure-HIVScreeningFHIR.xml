            </extension>
            <extension url="name">
               <valueString value="Has HIV Test Performed"></valueString>
            </extension>
            <extension url="statement">
               <valueString value="define &amp;quot;Has HIV Test Performed&amp;quot;:   exists ( [&amp;quot;Observation&amp;quot;: &amp;quot;Human Immunodeficiency Virus (HIV) Laboratory Test Codes (Ab and Ag)&amp;quot;]     union [&amp;quot;Observation&amp;quot;: &amp;quot;HIV 1 and 2 tests - Meaningful Use set&amp;quot;] ) HIVTest     where HIVTest.value is not null       and AgeInYearsAt(date from start of HIVTest.effective.toInterval()) in Interval[15, 65]       and HIVTest.effective.toInterval ( ) starts before end of &amp;quot;Measurement Period&amp;quot;       and ( HIVTest.status = &amp;#39;final&amp;#39;           or HIVTest.status = &amp;#39;amended&amp;#39;           or HIVTest.status = &amp;#39;corrected&amp;#39;       )"></valueString>
            </extension>
